package Ont::TRIPS;

$VERSION = '1.0';

use strict 'vars';
use warnings;

use Ont::Ontology;

our @ISA = qw(Ont::Ontology);


# (almost) full TRIPS ontology
my @data = (
	    ##ONT::TIME-OBJECT##,
	    ##ONT::REFERENTIAL-SEM##
	   );

sub new {
    my $class = shift;
    my $self = $class->SUPER::new(@data);
    bless $self, $class;
}

1;
